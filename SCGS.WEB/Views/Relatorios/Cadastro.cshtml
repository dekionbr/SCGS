@model List<SCGS.CORE.Entity.Consulta>
@{
    ViewBag.Title = "Relatórios Cadastro";

}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Relatório de Cadastro
        </h1>
        <ol class="breadcrumb">
            <li>
                <i class="fa fa-dashboard"></i>Dashboard
            </li>
            <li>
                <i class="fa fa-bar-chart-o"></i>Relatorios
            </li>
            <li class="active">
                <i class="fa fa-comments"></i>Cadastros
            </li>
        </ol>
    </div>
</div>

<div class="row">
    <div class="col-lg-12">
        <h2>Grafico de Cadastro</h2>
    </div>
</div>

<div class="row">
    <form role="form">
        <div class="col-lg-6">
            <div class="form-group">
                <label>Unidade</label>
                <select class="form-control ui-auto-unidade">
                    <option value="">Selecione a Unidade</option>                
                </select>
            </div>
        </div>
        <div class="col-lg-6">
            <div class="form-group">
                <label>Equipe</label>
                <select class="form-control ui-autocomplete" disabled>
                    <option value="">Selecione a Equipe</option>                    
                </select>
            </div>
        </div>        
    </form>
</div>
<p></p>
